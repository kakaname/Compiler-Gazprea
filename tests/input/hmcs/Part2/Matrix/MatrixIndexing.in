procedure main() returns integer {
  // vector decl
   // base
   integer[2,4] a = [[1,2,3,4],[5,6,7,8]];
   integer[3] v;
    integer i = 1;
    integer j = 1;

   loop while j < 3 {
    a[j,i] -> std_output;
    '\n' -> std_output;

    i = i+1;
    if i == 5 {
        j = j + 1;
        i = 1;
    } 
   } 
   
   v = a[1,1..3];
   v -> std_output;
    '\n' -> std_output;
    v =  a[1, [3,2,1]];
    v-> std_output;

  /* must have a return */
  return 0;
}