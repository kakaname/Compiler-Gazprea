procedure main() returns integer {
  // vector decl
   // base
   integer[2,3] a = [[1,2]];
   boolean[2,3] b = [[true,false]];
   character[2,3] c = [['a','b']];
   real[2,3] d = [[1.0,2.0]];

    a -> std_output;
    '\n' -> std_output;
    b -> std_output;
    '\n' -> std_output;
    c -> std_output;
    '\n' -> std_output;
    d -> std_output;

  /* must have a return */
  return 0;
}