procedure main() returns integer {
  // vector decl
   // base
   integer[3][3] a = [[1,2,3],[4,5,6],[7,8,9]];
   integer[3] b = [1,2,3];

    loop i in 1..3 {
        i -> std_output;
    }
    '\n' -> std_output;

    loop i in 1..3, j in 1..3 {
       a[i][j] -> std_output; 
    } 
    '\n' -> std_output;
    
    loop i in b {
        i -> std_output;
    }
    '\n' -> std_output;

    loop i in b, j in b {
       a[i][j] -> std_output; 
    } 
    '\n' -> std_output;
    
    loop i in b, j in 1..3 {
       a[i][j] -> std_output; 
    } 

  /* must have a return */
  return 0;
}