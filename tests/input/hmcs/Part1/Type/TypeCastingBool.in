procedure main() returns integer {

  integer x = 1;
  boolean y = as<boolean>(x);
  character z = 'a';
  y -> std_output;
   
  x = 0;
  y = as<boolean>(x);
  y -> std_output;

  y = as<boolean>(z);
  y -> std_output;

  z = null;
  y = as<boolean>(z);
  y -> std_output;

  /* must have a return */
  return 0;
}
