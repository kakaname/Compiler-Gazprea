procedure main() returns integer {

  // Integer
  var integer a = 10 % 2;   
  var integer d = 4;        
  var integer b = 11 % d;   
  var integer e = 5;       
  var integer c = e % b;    
  
  var real f = 11.0 % 2.2;
  var real j = 4.4;
  var real h = 12.1 % j;
  var real k = 5.0;
  var real i = k % h;

  a -> std_output;
  b -> std_output;
  c -> std_output;

  // Real
  f -> std_output;
  h -> std_output;
  i -> std_output;

  /* must have a return */
  return 0;
}