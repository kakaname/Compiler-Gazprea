procedure main() returns integer {
    // All logical expressions can only run on booleans
    
    // NOT
    not true -> std_output;
    not false -> std_output;

    // XOR
    true xor true -> std_output;
    false xor false -> std_output;
    true xor false -> std_output;
    false xor true -> std_output;

    // OR
    true or true -> std_output;
    false or false -> std_output;
    true or false -> std_output;
    false or true -> std_output;
    
    // AND
    true and true -> std_output;
    false and false -> std_output;
    true and false -> std_output;
    false and true -> std_output;

    return 0;
}