procedure main() returns integer {
  
  // Integer
  var integer a = 4 - 1;
  var integer b = 6 - a;
  var integer c = b - a;

  var real x = 4.4 - 1.1;
  var real y = 6.6 - x;
  var real z = y - x;
  
  a -> std_output;
  b -> std_output;
  c -> std_output;

  // Real
  x -> std_output;
  y -> std_output;
  z -> std_output;

  /* must have a return */
  return 0;
}