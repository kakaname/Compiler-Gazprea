procedure main() returns integer {

  // Integer
  var integer a = 1 + 1;
  a -> std_output;
  var integer b = 2 + a;
  b -> std_output;
  var integer c = a + b;
  c -> std_output;

  // Real
  var real x = 1.1 + 1.1;
  x -> std_output;
  var real y = 2.2 + x;
  y -> std_output;
  var real z = x + y;
  z -> std_output;

  /* must have a return */
  return 0;
}