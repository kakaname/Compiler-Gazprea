procedure get_integer(var integer x) {
    x <- std_input;
}
procedure get_character(var character x){
    x <- std_input;
}
procedure get_boolean(var boolean x){
    x <- std_input;
}
procedure get_real(var real x) {
    x <- std_input;
}
procedure main() returns integer {
/* Prints every number between 1 and 10 */
integer x;
character y;
boolean z;
real a;
character empty;
call get_integer(x); 
call get_character(empty);
call get_character(y); 
call get_character(empty);
call get_boolean(z); 
call get_character(empty);
call get_real(a); 

x -> std_output;
y -> std_output;
z -> std_output;
a -> std_output;

  /* must have a return */
  return 0;
}