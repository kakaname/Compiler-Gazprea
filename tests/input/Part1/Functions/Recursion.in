function factorial(integer) returns integer;

procedure main() returns integer {
  // testing for recursive function calls
  var out = factorial(5);
  out ->std_output;

  /* must have a return */
  return 0;
}

function factorial(integer x) returns integer {
    if(x==1){
        return 1;
    }else{
        integer b = factorial(x-1);

        return b*x;
    }
}