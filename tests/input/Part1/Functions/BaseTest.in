function times_two(integer x) returns integer = 2 * x;

function times_four(integer x) returns integer{
  return 4 * x;
}
procedure main() returns integer {
  // test both tests with single statement and 
  // block statements
  integer x = 2;
  integer out =   times_two(x);
  out -> std_output;
  out =   times_four(x);
  out -> std_output;

  /* must have a return */
  return 0;
}
