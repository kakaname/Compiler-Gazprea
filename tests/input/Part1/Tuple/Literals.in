procedure main() returns integer {

    var x = 5;
    tuple(integer) my_tuple = (10);
    tuple(integer, real r) tuple_var = (1, 2.1);
    var my_tuple_2 = (x);
    const my_tuple_3 = (x, true);

    my_tuple.1 -> std_output;
    tuple_var.1 -> std_output;
